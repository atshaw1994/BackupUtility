<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="Window.Background" Color="#191919"/>
    <SolidColorBrush x:Key="Window.Border" Color="#393c41"/>
    <SolidColorBrush x:Key="Window.Foreground" Color="#F1F1F1"/>

    <RadialGradientBrush x:Key="CaptionBar.Background.Inactive" RadiusX="0.5" RadiusY="1">
        <GradientStop Color="#2e3136" Offset="0.0"/>
        <GradientStop Color="#2e3136" Offset="0.5"/>
        <GradientStop Color="#393c41" Offset="1.0"/>
    </RadialGradientBrush>
    <SolidColorBrush x:Key="CaptionBar.Background.Active" Color="#393c41"/>
    <SolidColorBrush x:Key="CaptionBar.Foreground" Color="#F1F1F1"/>
    
    <SolidColorBrush x:Key="CaptionBar.Button.Background.Normal" Color="Transparent"/>
    <SolidColorBrush x:Key="CaptionBar.Button.Background.MouseOver" Color="#444444"/>
    <SolidColorBrush x:Key="CaptionBar.Button.Background.Pressed" Color="#3f3f3f"/>
    <SolidColorBrush x:Key="CaptionBar.Button.Background.Disabled" Color="#08ffffff"/>
    <LinearGradientBrush x:Key="CaptionBar.Button.Border.Normal" StartPoint="0.5,0" EndPoint="0.5,1.0">
        <GradientStop Color="#30ffffff" Offset="0.0"/>
        <GradientStop Color="#05000000" Offset="1.0"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="CaptionBar.Button.Border.MouseOver" StartPoint="0.5,0" EndPoint="0.5,1.0">
        <GradientStop Color="#3f4146" Offset="0.0"/>
        <GradientStop Color="#3f4146" Offset="0.5"/>
        <GradientStop Color="#313439" Offset="1.0"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="CaptionBar.Button.Border.Disabled" StartPoint="0.5,0" EndPoint="0.5,1.0">
        <GradientStop Color="#10ffffff" Offset="0.0"/>
        <GradientStop Color="#05000000" Offset="1.0"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="CaptionBar.Button.Border.Pressed" StartPoint="0.5,0" EndPoint="0.5,1.0">
        <GradientStop Color="#30ffffff" Offset="0.0"/>
        <GradientStop Color="#05000000" Offset="1.0"/>
    </LinearGradientBrush>
    <SolidColorBrush x:Key="CaptionBar.Button.Foreground.Normal" Color="#F1F1F1"/>
    <SolidColorBrush x:Key="CaptionBar.Button.Foreground.MouseOver" Color="#F1F1F1"/>
    <SolidColorBrush x:Key="CaptionBar.Button.Foreground.Pressed" Color="#F1F1F1"/>
    <SolidColorBrush x:Key="CaptionBar.Button.Foreground.Disabled" Color="#606060"/>

    <SolidColorBrush x:Key="Element.Background.Normal" Color="#383838"/>
    <SolidColorBrush x:Key="Element.Background.MouseOver" Color="#444444"/>
    <SolidColorBrush x:Key="Element.Background.Pressed" Color="#3f3f3f"/>
    <SolidColorBrush x:Key="Element.Background.Disabled" Color="#08ffffff"/>
    <LinearGradientBrush x:Key="Element.Border.Normal" StartPoint="0.5,0" EndPoint="0.5,1.0">
        <GradientStop Color="#30ffffff" Offset="0.0"/>
        <GradientStop Color="#05000000" Offset="1.0"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="Element.Border.MouseOver" StartPoint="0.5,0" EndPoint="0.5,1.0">
        <GradientStop Color="#3f4146" Offset="0.0"/>
        <GradientStop Color="#3f4146" Offset="0.5"/>
        <GradientStop Color="#313439" Offset="1.0"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="Element.Border.Disabled" StartPoint="0.5,0" EndPoint="0.5,1.0">
        <GradientStop Color="#10ffffff" Offset="0.0"/>
        <GradientStop Color="#05000000" Offset="1.0"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="Element.Border.Pressed" StartPoint="0.5,0" EndPoint="0.5,1.0">
        <GradientStop Color="#30ffffff" Offset="0.0"/>
        <GradientStop Color="#05000000" Offset="1.0"/>
    </LinearGradientBrush>
    <SolidColorBrush x:Key="Element.Foreground.Normal" Color="#F1F1F1"/>
    <SolidColorBrush x:Key="Element.Foreground.MouseOver" Color="#F1F1F1"/>
    <SolidColorBrush x:Key="Element.Foreground.Pressed" Color="#F1F1F1"/>
    <SolidColorBrush x:Key="Element.Foreground.Disabled" Color="#606060"/>

    <SolidColorBrush x:Key="TextBox.Background" Color="Transparent"/>
    <SolidColorBrush x:Key="TextBox.Border" Color="#10ffffff"/>

    <SolidColorBrush x:Key="ProgressBar.Track.Fill" Color="#40ffffff"/>
    <SolidColorBrush x:Key="ProgressBar.Track.Border" Color="#20ffffff"/>

    <CornerRadius x:Key="Element.CornerRadius" TopLeft="3" TopRight="3" BottomLeft="3" BottomRight="3"/>

</ResourceDictionary>