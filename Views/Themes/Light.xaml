<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="Window.Background" Color="#FFF"/>
    <SolidColorBrush x:Key="Window.Border" Color="#000"/>
    <SolidColorBrush x:Key="Window.Foreground" Color="#000"/>

    <SolidColorBrush x:Key="CaptionBar.Background.Active" Color="#BFCDDB"/>
    <SolidColorBrush x:Key="CaptionBar.Background.Inactive" Color="#fff"/>
    <SolidColorBrush x:Key="CaptionBar.Foreground" Color="#000000"/>

    <SolidColorBrush x:Key="Element.Background.Normal" Color="#dddddd"/>
    <SolidColorBrush x:Key="Element.Background.MouseOver" Color="#bee6fd"/>
    <SolidColorBrush x:Key="Element.Background.Pressed" Color="#c4e5f6"/>
    <SolidColorBrush x:Key="Element.Background.Disabled" Color="#f4f4f4"/>
    <SolidColorBrush x:Key="Element.Border.Normal" Color="#707070"/>
    <SolidColorBrush x:Key="Element.Border.Disabled" Color="#adb2b5"/>
    <SolidColorBrush x:Key="Element.Border.MouseOver" Color="#3c7fb1"/>
    <SolidColorBrush x:Key="Element.Border.Pressed" Color="#2c628b"/>
    <SolidColorBrush x:Key="Element.Foreground.Normal" Color="#1e1e1e"/>
    <SolidColorBrush x:Key="Element.Foreground.MouseOver" Color="#000"/>
    <SolidColorBrush x:Key="Element.Foreground.Pressed" Color="#000"/>
    <SolidColorBrush x:Key="Element.Foreground.Disabled" Color="#888888"/>

    <SolidColorBrush x:Key="ListBoxItem.Background.MouseOver" Color="#e5f3fb"/>
    <SolidColorBrush x:Key="ListBoxItem.Background.Selected" Color="#cbe8f6"/>
    <SolidColorBrush x:Key="ListBoxItem.Border.MouseOver" Color="#70c0e7"/>
    <SolidColorBrush x:Key="ListBoxItem.Border.Selected" Color="#26a0da"/>

    <SolidColorBrush x:Key="ListBox.Background" Color="#fff"/>
    <SolidColorBrush x:Key="ListBox.Border" Color="#abadb3"/>

    <SolidColorBrush x:Key="TextBox.Background" Color="#fff"/>
    <SolidColorBrush x:Key="TextBox.Border" Color="#d0d1d4"/>

    <SolidColorBrush x:Key="ProgressBar.Track.Fill" Color="Green"/>
    <SolidColorBrush x:Key="ProgressBar.Track.Border" Color="Black"/>

    <CornerRadius x:Key="Element.CornerRadius" TopLeft="3" TopRight="3" BottomLeft="3" BottomRight="3"/>

</ResourceDictionary>